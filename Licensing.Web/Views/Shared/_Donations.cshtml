@model IEnumerable<Licensing.Domain.Donations.Donation>

@if (Model != null)
{
    <div class="padded">
        @foreach (var donation in Model)
        {
            <div class="row padded-small fake-table">
                <div class="inline bold fake-table-cell">
                    @donation.Product.Name
                </div>
                <div class="inline fake-table-cell">
                    $@donation.Product.Price
                </div>
            </div>
        }
    </div>
}