@model IEnumerable<Licensing.Domain.AreasOfPractice.AreaOfPractice>

@if (Model != null)
{
    <div class="padded">
        @foreach (var areaOfPractice in Model)
        {
            <div class="row padded-small">
                @areaOfPractice.Option.Name
            </div>
        }
    </div>
}